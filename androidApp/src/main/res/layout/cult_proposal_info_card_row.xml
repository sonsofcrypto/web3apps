<?xml version="1.0" encoding="utf-8"?>
<com.sonsofcrypto.web3wallet.android.modules.cultproposal.views.CultProposalDetailStatusView
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:padding="?attr/padding">
    <TextView
        android:id="@+id/title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        style="@style/FontSubheadline"
        android:textColor="?attr/textSecondary"
        android:text="Hello"
        />
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="?attr/paddingHalf"
        />
    <TextView
        android:id="@+id/body"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        style="@style/FontSubheadline"
        android:textFontWeight="?attr/textFontWeightBold"
        android:text="Hello"
        />
</com.sonsofcrypto.web3wallet.android.modules.cultproposal.views.CultProposalDetailStatusView>